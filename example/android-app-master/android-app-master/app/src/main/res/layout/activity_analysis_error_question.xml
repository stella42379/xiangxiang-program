<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".AnalysisErrorQuestion">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@android:color/holo_blue_dark">

        <ImageView
            android:id="@+id/iv_black_analusis_error_question"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/go_back_selector"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@android:color/white"
            android:textSize="20dp"
            android:layout_centerInParent="true"
            android:text="错题解析"/>

    </RelativeLayout>
    <com.example.yinggutest.mianpage.fragment.everypractice.view.VoteSubmitViewPager
        android:id="@+id/vp_activity_analysis_error_question"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>

